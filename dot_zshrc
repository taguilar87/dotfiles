# Path to your oh-my-zsh installation.
export ZSH=$HOME/.oh-my-zsh

# Set name of the theme to load.
# Look in ~/.oh-my-zsh/themes/
# Optionally, if you set this to "random", it'll load a random theme each
# time that oh-my-zsh is loaded.
ZSH_THEME="robbyrussell"

# Uncomment the following line to use case-sensitive completion.
# CASE_SENSITIVE="true"

# Uncomment the following line to use hyphen-insensitive completion. Case
# sensitive completion must be off. _ and - will be interchangeable.
HYPHEN_INSENSITIVE="true"

# Uncomment the following line to disable bi-weekly auto-update checks.
# DISABLE_AUTO_UPDATE="true"

# Uncomment the following line to change how often to auto-update (in days).
# export UPDATE_ZSH_DAYS=13

# Uncomment the following line to disable colors in ls.
# DISABLE_LS_COLORS="true"

# Uncomment the following line to disable auto-setting terminal title.
# DISABLE_AUTO_TITLE="true"

# Uncomment the following line to enable command auto-correction.
# ENABLE_CORRECTION="true"

# Uncomment the following line to display red dots whilst waiting for completion.
COMPLETION_WAITING_DOTS="true"

# Uncomment the following line if you want to disable marking untracked files
# under VCS as dirty. This makes repository status check for large repositories
# much, much faster.
# DISABLE_UNTRACKED_FILES_DIRTY="true"

# Uncomment the following line if you want to change the command execution time
# stamp shown in the history command output.
# The optional three formats: "mm/dd/yyyy"|"dd.mm.yyyy"|"yyyy-mm-dd"
# HIST_STAMPS="mm/dd/yyyy"

# Would you like to use another custom folder than $ZSH/custom?
# ZSH_CUSTOM=/path/to/new-custom-folder

# Which plugins would you like to load? (plugins can be found in ~/.oh-my-zsh/plugins/*)
# Custom plugins may be added to ~/.oh-my-zsh/custom/plugins/
# Example format: plugins=(rails git textmate ruby lighthouse)
# Add wisely, as too many plugins slow down shell startup.
plugins=(git)

# User configuration
export EDITOR=vim

source $ZSH/oh-my-zsh.sh

# Vericred Aliases
alias be='bundle exec '
alias vc='cd ~/workspace/vericred'
alias hw='cd ~/workspace/howie'
alias hwa='cd ~/workspace/howie/api'
alias hwac='cd ~/workspace/howie/acceptance'
alias hwr='cd ~/workspace/howie/react'
alias pl='cd ~/workspace/plumbing'
alias pschem='cd ~/workspace/product_schemata'
alias dw='cd ~/workspace/data_warehouse'
alias gpr='git pr'
alias ll='ls -lAh'
alias dp='cd ~/workspace/data_pipeline'
alias tb='cd ~/workspace/toolbelt'
alias fun='cd ~/workspace/funnel'
alias vi='vim'
alias whereami='pwd'
alias gtower='gittower $(git rev-parse --show-toplevel)'
alias enrollments='cd ~/workspace/enrollments'

export AWS_SESSION_TOKEN_TTL=12h

# rbenv
eval "$(rbenv init -)"

# nodenv
export PATH="$HOME/.nodenv/bin:$PATH"
export PATH="./bin:$HOME/.rbenv/shims:$HOME/.config/yarn/global/node_modules/.bin:./node_modules/.bin:/usr/bin:/usr/local/bin:/bin:/usr/sbin:/sbin:/opt/X11/bin"
eval "$(nodenv init -)"

export HEROKU_ORGANIZATION=vericred-developers

export PATH="$(pyenv root)/shims:$PATH"

if [[ -f $HOME/.zshrc.local ]]; then
  source $HOME/.zshrc.local
fi

test -e "${HOME}/.iterm2_shell_integration.zsh" && source "${HOME}/.iterm2_shell_integration.zsh"

export PATH="/usr/local/opt/openjdk/bin:$PATH"
export PATH="$HOME/.cargo/bin:$PATH"
#export LIBRARY_PATH=$LIBRARY_PATH:/usr/local/opt/openssl/lib/
export JAVA_HOME="/usr/local/Cellar/openjdk/23.0.2/libexec/openjdk.jdk/Contents/Home"
export ES_JAVA_HOME="/usr/local/Cellar/openjdk/23.0.2/libexec/openjdk.jdk/Contents/Home"
export PATH="/Users/seanwilkins/workspace/x12_find:$PATH"
export PATH="/Library/Frameworks/R.framework/Resources:usr/bin/:$PATH"
export PATH="$HOME/Qt5.5.0/5.5/clang_64/bin:$PATH"
export PATH="/usr/local/opt/openssl@1.1/bin:$PATH"
export PATH="$HOME/.poetry/bin:$PATH"

export FZF_DEFAULT_OPTS='--height=70% --preview="cat {}" --preview-window=right:60%:wrap'
export FZF_DEFAULT_COMMAND='rg --files'
export FZF_CTRL_T_COMMAND='$FZF_DEFAULT_COMMAND'

[ -f ~/.fzf.zsh ] && source ~/.fzf.zsh

autoload -U +X bashcompinit && bashcompinit
complete -o nospace -C /usr/local/bin/terraform terraform

